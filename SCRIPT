#-----------------------------------------------
n = int(input())
nums = list(map(int, input().split()))
m = int(input())
#-----------------------------------------------
prefix = [0] * (n + 1)
S = 0
#-----------------------------------------------
for i in range(1, n + 1):
    prefix[i] = prefix[i - 1] + nums[i - 1]
query = list()
#-----------------------------------------------
for i in range(m):
    l, r = map(int, input().split())
    query.append((l, r))
for l, r in query:
    print(prefix[r] - prefix[l - 1])
